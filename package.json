{
  "name": "@knodes/typedoc-plugins",
  "version": "0.23.4",
  "description": "A monorepo containing all knodes-published TypeDoc plugins",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KnodesCommunity/typedoc-plugins.git"
  },
  "bugs": {
    "url": "https://github.com/KnodesCommunity/typedoc-plugins/issues"
  },
  "homepage": "https://github.com/KnodesCommunity/typedoc-plugins#readme",
  "author": {
    "email": "gerkin@knodes.org",
    "name": "GerkinDev"
  },
  "keywords": [
    "typedocplugin",
    "documentation"
  ],
  "workspaces": [
    "./packages/*"
  ],
  "scripts": {
    "build": "npm run tools:patch -- apply && node ./tools/s-projects \"npm run build -- --pretty\"",
    "build:clean": "npm run build:clean --workspaces",
    "build:watch": "npm run build && node ./tools/p-projects \"npm run build -- --watch --preserveWatchOutput --pretty\"",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "ci:clean-install": "npm clean-install --include-workspace-root",
    "ci:install": "npm install --include-workspace-root",
    "ci:test": "cross-env JEST_JUNIT_OUTPUT_NAME=junit.xml jest --config jest.config.ci.js --all --ci --reporters=default --reporters=jest-junit",
    "ci:test:coverage": "npm run ci:test -- --collectCoverage",
    "docs": "typedoc",
    "format:pkg": "format-package --write \"./package.json\" \"./packages/*/package.json\"",
    "lint": "eslint --ignore-path ./.eslintignore \"**/?(.)*.?([cm])[tj]s?(x)\"",
    "lint:md": "markdownlint .",
    "postprepare": "npm run postprepare:husky",
    "postprepare:husky": "run-script-os",
    "postprepare:husky:default": "[ -d ./node_modules/husky ] && husky install || true",
    "postprepare:husky:windows": "if exist './node_modules/husky' { husky install }",
    "precommit": "npm run tools:sync-proto -- --check && npm run tools:patch -- diff && npm run lint -- --cache && npm run lint:md && npm run build && npm run test -- --onlyChanged && npm run docs",
    "release": "run-script-os --",
    "release:default": "./tools/release.sh",
    "release:windows": "echo \"Unsupported platform for release. Run this command only from an UNIX-like environment.\"",
    "test": "jest",
    "test:coverage": "jest --config ./jest.config.coverage.js",
    "tools:bump-versions": "node tools/bump-versions",
    "tools:infer-next-version": "node tools/infer-next-version",
    "tools:patch": "node ./tools/typedoc-patcher --no-stash",
    "tools:sync-proto": "node tools/sync-proto.mjs"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "peerDependencies": {
    "typedoc": "^0.23.0"
  },
  "devDependencies": {
    "@alex_neo/jest-expect-message": "^1.0.5",
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@knodes/eslint-config": "^1.6.5",
    "@knodes/typedoc-plugin-code-blocks": "file:packages/plugin-code-blocks",
    "@knodes/typedoc-plugin-pages": "file:packages/plugin-pages",
    "@knodes/typedoc-plugintestbed": "file:packages/plugintestbed",
    "@knodes/typedoc-pluginutils": "file:packages/pluginutils",
    "@testing-library/jest-dom": "^5.16.4",
    "@types/concurrently": "^7.0.0",
    "@types/diff": "^5.0.2",
    "@types/fs-extra": "^11.0.0",
    "@types/jest": "^29.0.0",
    "@types/js-beautify": "^1.13.3",
    "@types/jsdom": "^21.0.0",
    "@types/lodash": "^4.14.182",
    "@types/lunr": "^2.3.4",
    "@types/marked": "^4.0.2",
    "@types/minimatch": "^5.0.0",
    "@types/mock-fs": "^4.13.1",
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.14",
    "@types/semver": "^7.3.9",
    "@types/tmp": "^0.2.3",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "chalk": "^4.1.2",
    "concurrently": "^8.0.0",
    "conventional-changelog-cli": "^3.0.0",
    "cross-env": "^7.0.3",
    "diff": "^5.1.0",
    "eslint": "^8.18.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.3.3",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-sort-export-all": "^1.2.2",
    "expect-type": "^0.16.0",
    "find-up": "^4.1.0",
    "format-package": "^7.0.0",
    "fs-monkey": "^1.0.3",
    "glob": "^10.0.0",
    "husky": "^8.0.1",
    "jest": "^29.0.0",
    "jest-cli": "^29.0.0",
    "jest-extended": "^4.0.0",
    "jest-junit": "^16.0.0",
    "js-beautify": "^1.14.0",
    "jsdom": "^21.0.0",
    "lodash": "^4.17.21",
    "lunr": "^2.3.0",
    "markdownlint-cli": "^0.34.0",
    "marked": "^4.0.12",
    "memfs": "^3.4.1",
    "minimatch": "^8.0.0",
    "nyc": "^15.1.0",
    "pkg-up": "^3.1.0",
    "rimraf": "^4.0.0",
    "run-script-os": "^1.1.6",
    "semver": "^7.3.5",
    "strip-ansi": "^6.0.1",
    "tmp": "^0.2.1",
    "ts-jest": "^29.0.0",
    "type-fest": "^3.0.0",
    "typedoc": "^0.23.28",
    "typescript": "^5.0.0",
    "unionfs": "^4.4.0",
    "yaml": "^2.1.1",
    "fastest-levenshtein": "^1.0.16"
  }
}
